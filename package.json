{
  "name": "shadcn-admin",
  "private": false,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "prepare": "husky && npx gitmoji -i",
    "release": "semantic-release",
    "openapi": "bun ./scripts/openapi.ts",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "npx @biomejs/biome lint --write",
    "preview": "vite preview",
    "format": "npx @biomejs/biome format --write",
    "knip": "knip",
    "i18n:extract": "npx i18next-cli extract",
    "i18n:sync": "npx i18next-cli sync",
    "i18n:status": "npx i18next-cli status"
  },
  "commitlint": {
    "extends": [
      "gitmoji"
    ],
    "parserPreset": {
      "parserOpts": {
        "headerPattern": "^(?:(\\u00a9|\\u00ae|[\\u2000-\\u3300]|\\ud83c[\\ud000-\\udfff]|\\ud83d[\\ud000-\\udfff]|\\ud83e[\\ud000-\\udfff])\\s*)?(\\w*)(?:\\((.*)\\))?!?:\\s*(.*)$",
        "headerCorrespondence": [
          "emoji",
          "type",
          "scope",
          "subject"
        ]
      }
    }
  },
  "gitmoji": {
    "autoAdd": false,
    "emojiFormat": "emoji",
    "scopePrompt": false,
    "messagePrompt": false,
    "capitalizeTitle": false,
    "gitmojisUrl": "https://gitmoji.dev/api/gitmojis"
  },
  "lint-staged": {
    "**/*.{js,jsx,tsx,ts,md,css,less,json}": [
      "npx @biomejs/biome check --write"
    ]
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-aspect-ratio": "^1.1.7",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-direction": "^1.1.1",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-menubar": "^1.1.16",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tailwindcss/vite": "^4.1.14",
    "@tanstack/react-router": "^1.132.31",
    "@tanstack/react-table": "^8.21.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "embla-carousel-react": "^8.6.0",
    "i18next": "^25.5.3",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.544.0",
    "next-themes": "^0.4.6",
    "openapi-fetch": "^0.14.1",
    "react": "^19.2.0",
    "react-day-picker": "^9.11.0",
    "react-dom": "^19.2.0",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.63.0",
    "react-i18next": "^16.0.0",
    "react-resizable-panels": "^3.0.6",
    "react-top-loading-bar": "^3.0.2",
    "recharts": "2.15.4",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.14",
    "tw-animate-css": "^1.4.0",
    "valtio": "^2.1.8",
    "vaul": "^1.1.2",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.5",
    "@commitlint/cli": "^19.8.1",
    "@faker-js/faker": "^10.0.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/exec": "^7.1.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.6",
    "@semantic-release/npm": "^12.0.2",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@tanstack/react-router-devtools": "^1.132.31",
    "@tanstack/router-plugin": "^1.132.31",
    "@types/node": "^24.6.2",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@vitejs/plugin-react-swc": "^4.1.0",
    "commitlint": "^20.1.0",
    "commitlint-config-gitmoji": "^2.3.1",
    "conventional-changelog-conventionalcommits": "^9.1.0",
    "gitmoji-cli": "^9.7.0",
    "husky": "^9.1.7",
    "i18next-cli": "^1.5.2",
    "knip": "^5.64.1",
    "lint-staged": "^16.2.3",
    "openapi-typescript": "^7.9.1",
    "semantic-release": "^24.2.9",
    "typescript": "~5.9.3",
    "vite": "^7.1.8"
  }
}
